#include <set>
#include <string>
#include <vector>
#include <iostream>
using namespace std;

vector<int> solution(vector<int> answers) {
    vector<int> ans(3,0);
        vector<int> answer;
    vector<int>sub;
    vector<int> person1 = { 1,2,3,4,5};//2
    vector<int> person2 = {2,1,2,3,2,4,2,5};//2
    vector <int> person3 = {3,3,1,1,2,2,4,4,5,5};
    for(int i = 0 ; i < answers.size();i++)
    {
        if(answers[i] == person1[i%5])
        {
            ans[0]++;
        }
        if(answers[i] == person2[i%8])
        {
            ans[1]++;
        }
        if(answers[i] == person3[i%10])
        {
            ans[2]++;
        }
    }
    for(int i = 0 ; i< ans.size();i++)   
    {
        if(ans[i] > 0)
        {
            answer.push_back(i+1);
        }
    }
    
    return answer;
}
