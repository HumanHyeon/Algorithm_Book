#include <string>
#include <vector>
#include <iostream>
#include <cmath>
using namespace std;

vector<int> solution(int brown, int yellow) {
    vector <int> answer;
    int 전체 = brown + yellow;
    //전체 = (노랑가로x2) * (노랑세로x2);
    //노랑의 약수를 구해야 함.
    vector <int> 노랑가로;
    vector <int> 노랑세로;
    int 전체가로;
    int 전체세로;
    for(int i = 1; i <=sqrt(yellow) ; i ++)
    {
        if(yellow % i == 0)
        {
            노랑가로.push_back(yellow/i);
            노랑세로.push_back(i);
        }
    }
    for(int i = 0; i < 노랑가로.size();i++)
    {
        cout << 노랑가로[i] << ' ' << 노랑세로[i] << endl;
        int 전체넓이 = (노랑가로[i]+2)*(노랑세로[i]+2);
        if(전체 == 전체넓이)
        {
            전체가로 = (노랑가로[i]+2);
            전체세로 = (노랑세로[i]+2);
            cout << 노랑가로[i] << ' ' <<노랑세로[i] << endl;
        }
    }
    answer.push_back(전체가로);
    answer.push_back(전체세로);
    return answer;
}
