코드를 짰으나, 프로그래머는 find 명령어가 안됐었습니다.
#include <string>
#include <vector>
#include <iostream>
using namespace std;
int sosu(string num)
{
    int count = 0;
    int a = num.length();
    vector <int> c;
    vector<int>d;
    vector<int>r;
    cout << "일의 자리 시작 : " << endl;
    //10의 자리 요소세기
    for (int i = 0; i < num.length(); i++)
    {
        int b = num[i] - '0';
        if (find(r.begin(), r.end(), b) - r.begin() == r.size())    //요소 찾기.
        {
            r.push_back(b);
            cout << " 일의자리 : " << b << endl;
            if (b != 0 && b != 1)
            {
                if (b == 2 || b == 3 || b == 5 || b == 7)   //소수찾기
                {
                    count++;
                }
            }
            cout << count << endl;
        }
    }
    cout << " 총 소수 : " << count << endl;
    cout << endl;

    //10의 자리 이상 요소 세기
    if (num.length() > 1)
    {
        //10의 자리 요소 세기
        cout << "십의자리 시작: " << endl;
        for (int i = 0; i < num.length(); i++)
        {
            for (int j = 0; j < num.length(); j++)
            {
                if (j == i);                        //중복되는 수 배제.
                else
                {
                    int a1 = 10 * (num[i] - '0');
                    a1 = a1 + (num[j] - '0');
                    if (find(c.begin(), c.end(), a1) - c.begin() == c.size())//요소 찾기.
                    {
                        if (a1 > 10)
                        {
                            c.push_back(a1);
                        }
                    }
                    //cout << a1 << endl;
                }
            }
        }

        //10의 자리 소수세기
        for (int i = 0; i < c.size(); i++)
        {
            cout << "십의자리 : ";
            cout << c[i] << endl;
            if (c[i] % 2 != 0 && c[i] % 3 != 0 && c[i] % 5 != 0 && c[i] % 7 != 0)
            {
                if (c[i] > 10);             //1의 자리인 수 배제.
                {
                    count++;
                }
                cout << count << endl;
            }
        }
        cout << " 총 소수 : " << count << endl;
        cout << endl;


        //백의 자리 요소 세기
        cout << "백의자리 시작 : " << endl;
        for (int i = 0; i < num.length(); i++)
        {
            for (int j = 0; j < num.length(); j++)
            {
                for (int w = 0; w < num.length(); w++)
                {
                    if (i == j);                            //중복요소 배제
                    else if (j == w);                       //중복요소 배제
                    else if (i == w);                       //중복요소 배제
                    else
                    {
                        int a1 = 100 * (num[i] - '0');
                        a1 = a1 + 10 * (num[j] - '0');
                        a1 = a1 + (num[w] - '0');
                        if (find(d.begin(), d.end(), a1) - d.begin() == d.size())//요소 찾기.
                        {
                            if (a1 > 100)               //1,10의 자리수 배제.
                            {
                                d.push_back(a1);
                            }
                        }
                    }
                }
            }
        }

        //백의 자리 소수세기
        for (int i = 0; i < d.size(); i++)
        {
            cout << "백의자리 :";
                cout << d[i] << endl;
                if (d[i] % 2 != 0 && d[i] % 3 != 0 && d[i] % 5 != 0 && d[i] % 7 != 0) 
                {
                    count++;
                    cout << count << endl;
                }
        }
        cout << "총 소수 : " << count << endl;
    }
    return count;
}

int main() {
    int answer = 0;
    int num = 0;
    string s1;
    getline(cin, s1);

    //1의 자리
    answer = sosu(s1);
    cout << "answer : " << answer;
}
