#include <string>
#include <vector>
#include <iostream>
#include <map>
#include <tuple>
using namespace std;

int solution(string skill, vector<string> skill_trees) {
    int answer = 0;
    map<char, int>알파벳;
    for (int i = 0; i < skill_trees.size(); i++)
    {
        map<char, int>::iterator iter;
        for (int j = 0; j < skill.size(); j++)
        {
            알파벳[skill[j]] = skill_trees[i].find(skill[j]);
        }
        for (iter = 알파벳.begin(); iter != 알파벳.end(); ++iter)
        {
            if (iter->second < (iter++)->second)
            {
                answer++;
            }
        }
        알파벳.clear();
    }
    return answer;
}
