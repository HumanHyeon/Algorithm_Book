#include <string>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

vector<string> solution(vector<string> record) {
    vector<string> answer;
    vector <string> userid;
    vector <string> 닉네임;
    string userid2;
    string 공백 = " ";
    int 횟수 = 0;
    int index = 0;
    while(횟수 < record.size())
    {
    int 공백2 = record[횟수].find(공백);
    string 명령어 = record[횟수].substr(0,공백2);  //명령어부분
    string 나머지 = record[횟수].substr(공백2+1);  //userid / 닉네임 부분
    //명령어 와 userid, 닉네임 분리
    int 공백_유저 = 나머지.find(공백);
    string userid3 = 나머지.substr(0,공백_유저);   //userid 부분
    string 닉네임2 = 나머지.substr(공백_유저+1);  //닉네임 부분
    //userid와  닉네임 분리
    if(명령어 == "Leave")
    {
        vector <string>::iterator it;
        for(it=userid.begin();it!=userid.end();it++)
        {
            if(*it == 나머지)
            {
                cout <<닉네임[index]<<"님께서 나가셨습니다."<<endl;
                닉네임.erase(닉네임.begin()+index);
                userid.erase(userid.begin()+index);
                break;
            }
            index++;
        }
    }
    else if(명령어 == "Enter")
    {
        닉네임.push_back(닉네임2);
        userid.push_back(userid3);
        cout << 닉네임2<<"님께서 들어오셨습니다."<<endl;
    }
    else if(명령어 == "Change")
    {
        vector <string>::iterator it;
        for(it=userid.begin();it!=userid.end();it++)
        {
            if(*it == userid3)
            {
                *it = 닉네임2;
                cout <<*it<<"님께서 들어오셨습니다."<<endl;
                break;
            }
            index++;
        }
    }
    else
    {
        cout<<"안돼요."<<endl;
    }       
    횟수++;
    }
    for(int i = 0 ;i < userid.size();i++)
    {
        cout << "유저아이디 "<<i<<" : " << userid[i] << endl;
    }
        for(int i = 0 ;i < 닉네임.size();i++)
    {
        cout << "닉네임 "<<i<<" : " << 닉네임[i] << endl;
    }
    return answer;
}
