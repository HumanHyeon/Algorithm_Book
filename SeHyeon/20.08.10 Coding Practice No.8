#define MOD 1234567
using namespace std;

long long cache[2000];

void dpCalc(long long n) {
    if (cache[n] == 0) {
        if (cache[n - 2] == 0)  dpCalc(n - 2);
        if (cache[n - 1] == 0)  dpCalc(n - 1);
        cache[n] = (cache[n - 2]  + cache[n - 1]) % MOD;
    }
}

long long solution(int n) {
    if (n == 1)  return 1;
    cache[0] = cache[1] = 1;
    dpCalc(n);

    return cache[n] % MOD;
}
